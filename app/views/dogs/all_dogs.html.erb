<h1>All Dogs</h1>

<div class="container">
  <div class="row">
      <% @dogs.each do |dog| %>
        <div class="col s12 m6">
          <div class="z-depth-5">
            <div class="card blue accent-1">
              <div class="card-content">
                <span class="card-title white-text"><%= dog.name %><br /><%= dog.breed %><br /><%= dog.description %></span>
              </div>
              <div class="card-action center blue darken-2">
                <%= link_to "Invite To Play!", new_dog_playdate_path(dog) %>
              </div>
            </div>
          </div>
        </div>
      <% end %>

  
  </div>
</div>
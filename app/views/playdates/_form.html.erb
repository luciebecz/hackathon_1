<%= form_for [@dog, @playdate] do |f| %>
<div>
  <%= f.label :location %>
  <%= f.text_field :location %>
</div>

<div>
  <%= f.label :start_time %>
  <%= f.text_field :start_time %>
</div>

<div>
  <%= f.label :end_time %>
  <%= f.text_field :end_time %>
</div>

<div>
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>

<%= f.submit "Submit", class:'btn teal lighten-2 black-text hoverable' %>
<% end %>
</div>